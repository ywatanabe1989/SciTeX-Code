    "\n",
    "# Connect to database\n",
    "sqlite_db.connect()\n",
    "print(\"Connected to SQLite3 database\")\n",
    "\n",
    "# Check database info\n",
    "db_info = sqlite_db.get_info()\n",
    "print(f\"Database info: {db_info}\")"
   ]
