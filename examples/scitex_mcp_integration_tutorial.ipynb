    "# Complete scientific workflow example\n",
    "scientific_pipeline = '''\n",
    "# ============ COMPLETE SCIENTIFIC PIPELINE ============\n",
    "import scitex as stx\n",
    "\n",
    "def scientific_analysis_pipeline(config_path='./ANALYSIS_CONFIG.yaml'):\n",
    "    \"\"\"Complete scientific analysis pipeline using SciTeX.\"\"\"\n",
    "    \n",
    "    # 1. Load configuration and data\n",
    "    config = stx.io.load(config_path)\n",
    "    raw_data = stx.io.load(config['paths']['raw_data'])\n",
    "    \n",
    "    print(f\"üìä Loaded {len(raw_data)} samples for analysis\")\n",
    "    \n",
    "    # 2. Data preprocessing with SciTeX\n",
    "    cleaned_data = raw_data.dropna()\n",
    "    \n",
    "    # Enhanced descriptive statistics\n",
    "    stats_summary = stx.stats.describe(cleaned_data.select_dtypes(include=[float, int]))\n",
    "    print(f\"üìà Data summary: {len(stats_summary)} variables analyzed\")\n",
    "    \n",
    "    # 3. Statistical analysis\n",
    "    # Group comparisons with multiple comparison correction\n",
    "    groups = cleaned_data['group'].unique()\n",
    "    comparison_results = {}\n",
    "    \n",
    "    for i, group1 in enumerate(groups):\n",
    "        for group2 in groups[i+1:]:\n",
    "            data1 = cleaned_data[cleaned_data['group'] == group1]['measurement']\n",
    "            data2 = cleaned_data[cleaned_data['group'] == group2]['measurement']\n",
    "            \n",
    "            # Robust statistical test\n",
    "            result = stx.stats.brunner_munzel_test(data1, data2)\n",
    "            comparison_results[f\"{group1}_vs_{group2}\"] = {\n",
    "                'statistic': result['statistic'],\n",
    "                'p_value': result['p_value'],\n",
    "                'significance': stx.stats.p2stars(result['p_value'])\n",
    "            }\n",
    "    \n",
    "    # Multiple comparison correction\n",
    "    p_values = [r['p_value'] for r in comparison_results.values()]\n",
    "    corrected_p = stx.stats.bonferroni_correction(p_values)\n",
    "    \n",
    "    # 4. Visualization with enhanced features\n",
    "    fig, axes = stx.plt.subplots(2, 2, figsize=(12, 10))\n",
    "    \n",
    "    # Distribution plot\n",
    "    axes[0, 0].sns_boxplot(data=cleaned_data, x='group', y='measurement')\n",
    "    axes[0, 0].set_xyt('Group', 'Measurement', 'Group Distributions')\n",
    "    \n",
    "    # Correlation heatmap\n",
