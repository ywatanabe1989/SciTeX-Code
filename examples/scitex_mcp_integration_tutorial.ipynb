    "    \n",
    "    # Enhanced descriptive statistics with SciTeX\n",
    "    desc_stats = stx.stats.describe(data.select_dtypes(include=[float, int]))\n",
    "    print(\"üìä Comprehensive Descriptive Statistics:\")\n",
    "    for col, stats in desc_stats.items():\n",
    "        print(f\"  {col}: mean={stats['mean']:.3f}, std={stats['std']:.3f}\")\n",
    "    \n",
    "    # Statistical tests with enhanced reporting\n",
    "    group_a = data[data['group'] == 'A']['measurement']\n",
    "    group_b = data[data['group'] == 'B']['measurement']\n",
    "    \n",
    "    # Enhanced t-test with effect size\n",
    "    test_result = stx.stats.brunner_munzel_test(group_a, group_b)\n",
    "    print(f\"\\nüß™ Statistical Test Results:\")\n",
    "    print(f\"  Test statistic: {test_result['statistic']:.3f}\")\n",
    "    print(f\"  P-value: {test_result['p_value']:.6f}\")\n",
    "    print(f\"  Significance: {stx.stats.p2stars(test_result['p_value'])}\")\n",
    "    \n",
    "    # Correlation analysis with permutation testing\n",
    "    corr_results = {}\n",
    "    numeric_cols = data.select_dtypes(include=[float, int]).columns\n",
    "    for i, col1 in enumerate(numeric_cols):\n",
    "        for col2 in numeric_cols[i+1:]:\n",
    "            corr_result = stx.stats.corr_test(data[col1], data[col2])\n",
